<!DOCTYPE html>

<html>
<head>
	<meta name="viewport" content="width=device-width" />
	<title>@ViewData["Title"] | Diagnostic Management System</title>
	<environment include="Development">
		<link rel="stylesheet" href="~/lib/twitter-bootstrap/css/bootstrap.css" asp-append-version="true" />
		<link rel="stylesheet" href="~/lib/jqueryui/jquery-ui.min.css" asp-append-version="true" />
		<link rel="stylesheet" href="~/lib/jqueryui/themes/start/jquery-ui.min.css" asp-append-version="true" />
		<link rel="stylesheet" href="~/lib/jquery-ui-timepicker-addon/jquery-ui-timepicker-addon.min.css" asp-append-version="true" />
		<link rel="stylesheet" href="~/lib/summernote/summernote-lite.min.css" asp-append-version="true" />
		<link rel="stylesheet" href="~/lib/font-awesome/css/all.min.css" asp-append-version="true" />
		<link rel="stylesheet" href="~/css/MyStyleSheet.css" asp-append-version="true" />
		<link rel="stylesheet" href="~/css/Site.css" asp-append-version="true" />
	</environment>
	<environment exclude="Development">
		<link rel="stylesheet"
			  href="https://cdn.jsdelivr.net/npm/bootstrap@4.6.0/dist/css/bootstrap.min.css"
			  asp-fallback-href="~/lib/twitter-bootstrap/css/bootstrap.min.css"
			  asp-fallback-test-class="sr-only"
			  asp-fallback-test-property="position"
			  asp-fallback-test-value="absolute"
			  integrity="sha384-B0vP5xmATw1+K9KRQjQERJvTumQW0nPEzvF6L/Z6nronJ3oUOFUFpCjEUQouq2+l"
			  crossorigin="anonymous">
		<link rel="stylesheet"
			  href="https://cdnjs.cloudflare.com/ajax/libs/jqueryui/1.12.1/jquery-ui.min.css"
			  asp-fallback-href="~/lib/jqueryui/jquery-ui.min.css"
			  asp-fallback-test-class="ui-helper-hidden-accessible"
			  asp-fallback-test-property="position"
			  asp-fallback-test-value="absolute"
			  integrity="sha512-aOG0c6nPNzGk+5zjwyJaoRUgCdOrfSDhmMID2u4+OIslr0GjpLKo7Xm0Ao3xmpM4T8AmIouRkqwj1nrdVsLKEQ=="
			  crossorigin="anonymous" />
		<link rel="stylesheet" href="~/lib/jqueryui/themes/start/jquery-ui.min.css" asp-append-version="true" />
		<link rel="stylesheet" href="~/lib/jquery-ui-timepicker-addon/jquery-ui-timepicker-addon.min.css" asp-append-version="true" />
		<link rel="stylesheet" href="~/lib/summernote/summernote-lite.min.css" asp-append-version="true" />
		<link rel="stylesheet" href="~/lib/font-awesome/css/all.min.css" asp-append-version="true" />
		<link rel="stylesheet" href="~/css/Site.css" asp-append-version="true" />
		<link rel="stylesheet" href="~/css/MyStyleSheet.min.css" asp-append-version="true" />
	</environment>
</head>
<body>
	<partial name="_HeaderAuthenticatedPartialView" />
	<partial name="_LayoutSideMenuAuthenticated" />
	<main role="main" id="main1">
		<div class="modal fade " tabindex="-1" role="dialog" aria-labelledby="mySmallModalLabel" aria-hidden="true" id="mi-modal">
			<div class="modal-dialog modal-static  modal-dialog-centered">
				<div class="modal-content messageBoxColor">
					<div class="modal-header">
						<i class="fa fa-save fa-2x mr-2 text-success"></i>
						<h3 class="modal-title text-primary" id="myModalLabel">Are you sure save record ?</h3>
						<button type="button" class="close" data-dismiss="modal" aria-label="Close">
							<span aria-hidden="true">&times;</span>
						</button>
					</div>

					<div class="modal-footer">
						<button href="#" type="button" class="btn btn-outline-success" id="modal-btn-si" value="Yes">Yes</button>
						<button href="#" type="button" class="btn btn-outline-danger" id="modal-btn-no" value="No">No</button>
					</div>
				</div>
			</div>
		</div>
		<!-- Modal Edit User-->
		<div class="modal fade" id="ModelIdEditUser" data-backdrop="static" role="dialog" tabindex="-1">
			<div class="modal-dialog modal-dialog-centered modal-dialog-scrollable modal-xl" role="document">
				<div class="modal-content">
					<div class="modal-header">
						<h3 id="title" class="modal-title text-center">Edit User Details</h3>
						<a href="#" class="close" data-dismiss="modal">x</a>
					</div>
					<div class="modal-body" id="ModelBodyIdEditUser">
					</div>
				</div>
			</div>
		</div>
		@RenderBody()
	</main>
	@*<partial name="_footer" />*@
<environment include="Development">
	<script src="~/lib/jquery/jquery.min.js" asp-append-version="true"></script>
	<script src="~/lib/jqueryui/jquery-ui.min.js" asp-append-version="true"></script>
	<script src="~/lib/jquery-validate/jquery.validate.js" asp-append-version="true"></script>
	<script src="~/lib/jquery-validation-unobtrusive/jquery.validate.unobtrusive.js" asp-append-version="true"></script>
	@*<script src="~/lib/popper.js/cjs/popper.min.js" asp-append-version="true"></script>*@
	<script src="~/lib/jquery-ui-timepicker-addon/jquery-ui-timepicker-addon.min.js" asp-append-version="true"></script>
	<script src="~/lib/twitter-bootstrap/js/bootstrap.min.js" asp-append-version="true"></script>
	@*<script src="~/lib/Chart.js/Chart.js" asp-append-version="true"></script>*@
	<script src="~/js/site.js" asp-append-version="true"></script>
	<script src="~/js/Custom.js" asp-append-version="true"></script>
	<script src="~/lib/summernote/summernote-lite.min.js" asp-append-version="true"></script>
	<script src="https://www.gstatic.com/charts/loader.js"></script>	
</environment>

<environment exclude="Development">
	<script src="https://code.jquery.com/jquery-3.6.0.min.js"
			asp-fallback-src="~/lib/jquery/jquery.min.js"
			asp-fallback-test="window.jQuery"
			integrity="sha256-/xUj+3OJU5yExlq6GSYGSHk7tPXikynS7ogEvDej/m4="
			crossorigin="anonymous">
	</script>
	<script src="https://code.jquery.com/ui/1.12.1/jquery-ui.min.js"
			asp-fallback-src="~/lib/jqueryui/jquery-ui.min.js"
			asp-fallback-test="!!window.jQuery.ui && !!window.jQuery.ui.version"
			integrity="sha256-VazP97ZCwtekAsvgPBSUwPFKdrwD3unUfSGVYrahUqU="
			crossorigin="anonymous">
	</script>
	<script src="~/lib/jquery-validate/jquery.validate.min.js" asp-append-version="true"></script>
	<script src="~/lib/jquery-validation-unobtrusive/jquery.validate.unobtrusive.min.js" asp-append-version="true"></script>
	<script src="~/lib/jquery-ui-timepicker-addon/jquery-ui-timepicker-addon.min.js" asp-append-version="true"></script>
	<script src="https://cdn.jsdelivr.net/npm/popper.js@1.16.1/dist/umd/popper.min.js"
			asp-fallback-src="~/lib/popper.js/cjs/popper.min.js"
			asp-fallback-test="window.Popper"
			integrity="sha384-9/reFTGAW83EW2RDu2S0VKaIzap3H66lZH81PoYlFhbGU+6BZp6G7niu735Sk7lN"
			crossorigin="anonymous">
	</script>
	<script src="https://cdn.jsdelivr.net/npm/bootstrap@4.6.0/dist/js/bootstrap.min.js"
			asp-fallback-src="~/lib/twitter-bootstrap/js/bootstrap.min.js"
			asp-fallback-test="window.jQuery && window.jQuery.fn && window.jQuery.fn.modal"
			integrity="sha384-+YQ4JLhjyBLPDQt//I+STsc9iw4uQqACwlvpslubQzn4u2UU2UFM80nGisd026JF"
			crossorigin="anonymous">
	</script>
	@*<script src="~/lib/Chart.js/chart.min.js" asp-append-version="true"></script>*@
	<script src="~/js/site.min.js" asp-append-version="true"></script>
	<script src="~/js/Custom.min.js" asp-append-version="true"></script>
	<script src="~/lib/summernote/summernote-lite.min.js" asp-append-version="true"></script>
	<script src="https://www.gstatic.com/charts/loader.js"></script>	
</environment>
	@RenderSection("scripts", required: false)
</body>
</html>
<script>
	var AddEditExpenses = function (itemId) {
		var url = "/Master/CustomerEditUserDetailRecord?itemId=" + itemId;

		$("#ModelBodyIdEditUser").load(url, function (result) {
			$("#ModelBodyIdEditUser").html(result);
			$("#ModelIdEditUser").modal("show");
		});

	}
</script>
