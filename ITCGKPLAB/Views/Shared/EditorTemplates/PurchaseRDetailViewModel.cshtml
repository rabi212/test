@model PurchaseRDetailViewModel
<tr>
	<td class="text-center">
		@*<a href="#" id="Delbutton" type="button" onclick="deleteRow(this)" class="mt-1"><i class="fas fa-times fa-1x icon-color-red" ></i></a>*@
		<input id="Delbutton" type="button" onclick="deleteRow(this)" value="Del" class="btn btn-info btn-sm" style="width:auto;" />
	</td>
	<td>
		<input asp-for="TempSrNo" class="form-control " readonly />
	</td>
	<td>
		<input asp-for="ItemCode" class="form-control  " hidden />
		<input asp-for="STItemName" class="form-control   OpenItemDetailRecord" onfocus="FindRowIndex(this); ItemSearchRecord();" onblur="FilterItemBatchNo();" />
	</td>
	<td>
		<input asp-for="BatchNo" class="form-control  BatchDetailRecord" onfocus="BatchSearchRecord();" />
	</td>
	<td>
		<input asp-for="ExpDate" class="form-control " placeholder="dd/mm/yyyy" readonly />
	</td>
	<td>
		<input asp-for="CasePcs" class="form-control  text-sm-right" onfocus="FindRowIndex(this)" onkeypress="return isNumber(event)" onblur="ConvertTwoDecimal(this);" />
	</td>
	<td>
		<input asp-for="OnFreeCase" class="form-control  text-sm-right" onfocus="FindRowIndex(this)" onkeypress="return isNumber(event)" onblur="ConvertTwoDecimal(this);" />
	</td>
	<td>
		<input asp-for="FreePcs" class="form-control  text-sm-right" onfocus="FindRowIndex(this)" onkeypress="return isNumber(event)" onblur="ConvertTwoDecimal(this);OpenTotalPcs();" />
	</td>
	<td>
		<input asp-for="TotalPcs" class="form-control  text-sm-right" onfocus="FindRowIndex(this)" onkeypress="return isNumber(event)" onblur="ConvertTwoDecimal(this);" readonly />
	</td>
	<td>
		<input asp-for="PurRate" class="form-control  text-sm-right" onfocus="FindRowIndex(this)" onkeypress="return isNumber(event)" onblur="ConvertTwoDecimal(this); TotalPurchaseAmt();" />
	</td>
	<td>
		<input asp-for="PurAmt" class="form-control  text-sm-right" onfocus="FindRowIndex(this)" onkeypress="return isNumber(event)" onblur="ConvertTwoDecimal(this);" readonly />
	</td>
	<td>
		<input asp-for="DiscPer1" class="form-control  text-sm-right" onfocus="FindRowIndex(this)" onkeypress="return isNumber(event)" onblur="ConvertTwoDecimal(this); TotalDiscountAmt();" />
	</td>
	<td>
		<input asp-for="DiscAmt1" class="form-control  text-sm-right" onfocus="FindRowIndex(this)" onkeypress="return isNumber(event)" onblur="ConvertTwoDecimal(this); TotalDiscountAmt();" />
	</td>
	<td>
		<input asp-for="GSTPer" class="form-control  text-sm-right" onfocus="FindRowIndex(this)" onkeypress="return isNumber(event)" onblur="ConvertTwoDecimal(this);" />
	</td>
	<td>
		<input asp-for="MRP" class="form-control  text-sm-right" onkeypress="return isNumber(event)" onblur="ConvertTwoDecimal(this); NetPurchaseAmount(); GrandTotalAmtCount();" />
	</td>
	<td>
		<input asp-for="SaleRate" class="form-control  text-sm-right" onfocus="FindRowIndex(this)" onkeypress="return isNumber(event)" onblur="ConvertTwoDecimal(this);" readonly />
	</td>
	<td>
		<input asp-for="NetPurRate" class="form-control  text-sm-right" onfocus="FindRowIndex(this)" onkeypress="return isNumber(event)" onblur="ConvertTwoDecimal(this);" readonly />
	</td>
	<td hidden>
		<input asp-for="DiscPer2" title="Disc 2 %" class="form-control  text-sm-right" onfocus="FindRowIndex(this)" onkeypress="return isNumber(event)" onblur="ConvertTwoDecimal(this);" />
		<input asp-for="DiscAmt2" title="Disc 2 Amt " class="form-control  text-sm-right" onfocus="FindRowIndex(this)" onkeypress="return isNumber(event)" onblur="ConvertTwoDecimal(this);" />
		<input asp-for="DiscPer3" title="Disc 3 %" class="form-control  text-sm-right" onfocus="FindRowIndex(this)" onkeypress="return isNumber(event)" onblur="ConvertTwoDecimal(this);" />
		<input asp-for="DiscAmt3" title="Disc 3 Amt" class="form-control  text-sm-right" onfocus="FindRowIndex(this)" onkeypress="return isNumber(event)" onblur="ConvertTwoDecimal(this);" />
		<input asp-for="TotalDiscAmt" title="Total Disc Amt" class="form-control   text-right" onfocus="FindRowIndex(this)" onkeypress="return isNumber(event)" onblur="ConvertTwoDecimal(this);" />
		<input asp-for="CGSTAmt" title="CGST Amt." class="form-control   text-right" onfocus="FindRowIndex(this)" onkeypress="return isNumber(event)" onblur="ConvertTwoDecimal(this);" />
		<input asp-for="SGSTAmt" title="SGST Amt" class="form-control   text-right" onfocus="FindRowIndex(this)" onkeypress="return isNumber(event)" onblur="ConvertTwoDecimal(this);" />
		<input asp-for="IGSTAmt" title="IGST Amt" class="form-control   text-right" onfocus="FindRowIndex(this)" onkeypress="return isNumber(event)" onblur="ConvertTwoDecimal(this);" />
		<input asp-for="MRPAmt" title="MRP Amt." class="form-control  text-sm-right" onfocus="FindRowIndex(this)" onkeypress="return isNumber(event)" onblur="ConvertTwoDecimal(this);" />
		<input asp-for="NetPurAmt" title="Net Pur Amt" class="form-control  text-sm-right" onfocus="FindRowIndex(this)" onkeypress="return isNumber(event)" onblur="ConvertTwoDecimal(this);" />
		<input asp-for="UnitCase" title="Unit Case" class="form-control   text-right" onfocus="FindRowIndex(this)" onkeypress="return isNumber(event)" onblur="ConvertTwoDecimal(this);" />
		<input asp-for="RecordType" title="RecordType" class="form-control   text-right" onfocus="FindRowIndex(this)"/>
	</td>
</tr>
