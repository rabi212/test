@model OpenItemMasterDetailViewModel
<tr>
	<td class="text-center">
		@*<a href="#" id="Delbutton" type="button" onclick="deleteRow(this)" class="mt-1"><i class="fas fa-times fa-1x icon-color-red" ></i></a>*@
		<input id="Delbutton" type="button" onclick="deleteRow(this)" value="Del" class="btn btn-info btn-sm" style="width:auto;" />
	</td>
	<td>
		<input asp-for="TempSrNo" class="form-control " readonly/>
	</td>
	<td>
		<input asp-for="ItemCode" class="form-control " hidden />		
		<input asp-for="ItemName" class="form-control  OpenItemDetailRecord" onfocus="FindRowIndex(this); ItemSearchRecord();" />
	</td>
	<td>
		<input asp-for="BatchNo" class="form-control  " />
	</td>
	<td>
		<input asp-for="ExpDate" class="form-control " placeholder="dd/mm/yyyy" />
	</td>
	<td>
		<input asp-for="CasePcs" class="form-control  text-sm-right" onfocus="FindRowIndex(this)" onkeypress="return isNumber(event)" onblur="ConvertTwoDecimal(this);" />
	</td>
	<td>
		<input asp-for="FreePcs" class="form-control  text-sm-right" onfocus="FindRowIndex(this)" onkeypress="return isNumber(event)" onblur="ConvertTwoDecimal(this);OpenTotalPcs();" />
	</td>
	<td>
		<input asp-for="TotalPcs" class="form-control  text-sm-right" onfocus="FindRowIndex(this)" onkeypress="return isNumber(event)" onblur="ConvertTwoDecimal(this);" readonly />
	</td>
	<td>
		<input asp-for="GSTPer" class="form-control  text-sm-right" onfocus="FindRowIndex(this)" onkeypress="return isNumber(event)" onblur="ConvertTwoDecimal(this);" />
	</td>
	<td>
		<input asp-for="PurRate" class="form-control  text-sm-right" onfocus="FindRowIndex(this)" onkeypress="return isNumber(event)" onblur="ConvertTwoDecimal(this);" />
	</td>
	<td>
		<input asp-for="MRP" class="form-control  text-sm-right" onkeypress="return isNumber(event)" onblur="ConvertTwoDecimal(this); OpenItemPurMRPAmount();" />
	</td>
	<td>
		<input asp-for="SaleRate" class="form-control  text-sm-right" onfocus="FindRowIndex(this)" onkeypress="return isNumber(event)" onblur="ConvertTwoDecimal(this); TotalPurMRPAmtCount();" readonly />		
	</td>
	<td hidden >
		<input asp-for="MRPAmt" class="form-control  text-sm-right" onfocus="FindRowIndex(this)" onkeypress="return isNumber(event)" onblur="ConvertTwoDecimal(this);" />
		<input asp-for="NetPurRate" class="form-control  text-sm-right" onkeypress="return isNumber(event)" onblur="ConvertTwoDecimal(this);" />
		<input asp-for="NetPurAmt" class="form-control  text-sm-right" onfocus="FindRowIndex(this)" onkeypress="return isNumber(event)" onblur="ConvertTwoDecimal(this);" />
		<input asp-for="PurAmt" class="form-control  text-sm-right" onkeypress="return isNumber(event)" onblur="ConvertTwoDecimal(this);" />
		<input asp-for="CessPer" class="form-control  text-sm-right" />
		<input asp-for="CessAmt" class="form-control  text-sm-right" />
		<input asp-for="UnitCase" class="form-control  text-right" />
	</td>
</tr>
